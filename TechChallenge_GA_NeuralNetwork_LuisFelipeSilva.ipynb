    "plt.title('Evolu√ß√£o da Acur√°cia')\n",
    "plt.xlabel('Gera√ß√£o')\n",
    "plt.ylabel('Acur√°cia')\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
